#include <cstdlib>
#include <iostream>
#include <string>

#include "shell.h"
#include "path_resolver.h"

using std::cin;
using std::string;

int main(int argc, char* argv[], char* envp[]) {
	Command cmd;
	cmd.filename = "/bin/ls";
	cmd.argv = {"ls", "/home/stuart/dev/shell/shell"};
	cmd.envp = {};

	Shell shell(PathResolver("/bin"));
	shell.Execute(cmd, stdout);

	return EXIT_SUCCESS;
}
